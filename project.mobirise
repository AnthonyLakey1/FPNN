          "_customHTML": "<section class=\"footer1\" group=\"Footer\" mbr-class=\"{'mbr-reveal': reveal, 'mbr-parallax-background': bg.parallax}\" always-bottom global once=\"footer\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n        <input type=\"checkbox\" title=\"Reveal effect\" name=\"reveal\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Brand\" name=\"showBrand\" checked>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\" checked>\n        <input type=\"range\" title=\"Logo Size\" inline name=\"logoSize\" min=\"2\" max=\"5\" step=\"0.1\" value=\"5\" condition=\"showLogo\">\n        <input type=\"checkbox\" title=\"Show Bottom\" name=\"showBottom\" checked>\n        <select name=\"countMenu\" title=\"Count Menu\">\n            <option value=\"0\">None</option>\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n        </select>\n        <input type=\"range\" title=\"Item 1\" name=\"countItem1\" min=\"1\" max=\"10\" step=\"1\" value=\"4\" condition=\"countMenu > '0'\">\n        <input type=\"range\" title=\"Item 2\" name=\"countItem2\" min=\"1\" max=\"10\" step=\"1\" value=\"3\" condition=\"countMenu > '1'\">\n        <input type=\"range\" title=\"Item 3\" name=\"countItem3\" min=\"1\" max=\"10\" step=\"1\" value=\"3\" condition=\"countMenu > '2'\">\n        <input type=\"range\" title=\"Item 4\" name=\"countItem4\" min=\"1\" max=\"10\" step=\"1\" value=\"3\" condition=\"countMenu > '3