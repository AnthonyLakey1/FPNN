          "_customHTML": "<section class=\"footer1\" group=\"Footer\" mbr-class=\"{'mbr-reveal': reveal, 'mbr-parallax-background': bg.parallax}\" always-bottom global once=\"footer\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n        <input type=\"checkbox\" title=\"Reveal effect\" name=\"reveal\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Brand\" name=\"showBrand\" checked>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\" checked>\n        <input type=\"range\" title=\"Logo Size\" inline name=\"logoSize\" min=\"2\" max=\"5\" step=\"0.1\" value=\"5\" condition=\"showLogo\">\n        <input type=\"checkbox\" title=\"Show Bottom\" name=\"showBottom\" checked>\n        <select name=\"countMenu\" title=\"Count Menu\">\n            <option value=\"0\">None</option>\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n        </select>\n        <input type=\"range\" title=\"Item 1\" name=\"countItem1\" min=\"1\" max=\"10\" step=\"1\" value=\"4\" condition=\"countMenu > '0'\">\n        <input type=\"range\" title=\"Item 2\" name=\"countItem2\" min=\"1\" max=\"10\" step=\"1\" value=\"3\" condition=\"countMenu > '1'\">\n        <input type=\"range\" title=\"Item 3\" name=\"countItem3\" min=\"1\" max=\"10\" step=\"1\" value=\"3\" condition=\"countMenu > '2'\">\n        <input type=\"range\" title=\"Item 4\" name=\"countItem4\" min=\"1\" max=\"10\" step=\"1\" value=\"3\" condition=\"countMenu > '3'\">\n        <input type=\"color\" title=\"Line\" value=\"#38b6ff\" name=\"lineColor\" condition=\"showBottom\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"../_images/background.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#232323\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"align-center\" mbr-class=\"{'container' : !fullWidth, 'container-fluid' : fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-12 col-md-12\" mbr-if=\"showBrand || showLogo\">\n                <div class=\"brand_wrap\">\n                     <img class=\"footer_logo\" mbr-if=\"showLogo\" src=\"@PROJECT_PATH@/assets/images/untitled-design-96x96.png\" alt=\"FPNN Logo\" mbr-style=\"{'height': logoSize + 'rem'}\" title>\n                   <p class=\"footer_caption mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showBrand\" data-app-selector=\".footer_caption\">Forever Positive News Network™</p>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-3\" mbr-if=\"countMenu > 0\">\n                <div class=\"footer_wrap mbr-white\">\n                    <h4 class=\"mbr-fonts-style mbr-semibold footer_title\" mbr-theme-style=\"display-7\" data-app-selector=\".footer_title\">About</h4>\n                    